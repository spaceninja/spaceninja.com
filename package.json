{
  "name": "spaceninja.com",
  "version": "10.0.0",
  "description": "",
  "author": "Scott Vandehey",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn watch",
    "build": "run-p build:*",
    "build:content": "eleventy",
    "build:css": "sass src:dist",
    "watch": "run-p watch:*",
    "watch:content": "eleventy --serve --quiet",
    "watch:css": "sass --watch src:dist",
    "lint": "run-s lint:js lint:css lint:prettier",
    "lint:check": "run-s lint:*:check",
    "lint:js": "eslint . --fix",
    "lint:js:check": "eslint .",
    "lint:css": "stylelint --fix '**/*.{css,scss}'",
    "lint:css:check": "stylelint '**/*.{css,scss}'",
    "lint:prettier": "prettier . --write",
    "lint:prettier:check": "prettier . --check"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-img": "3.1.0",
    "@11ty/eleventy-navigation": "0.3.5",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "eleventy-plugin-time-to-read": "1.3.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "luxon": "3.4.3",
    "markdown-it-anchor": "8.6.7",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "sass": "1.67.0",
    "stylelint": "15.10.3",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-spaceninja": "13.0.1"
  }
}
