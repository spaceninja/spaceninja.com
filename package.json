{
  "name": "spaceninja.com",
  "version": "10.0.0",
  "description": "",
  "author": "Scott Vandehey",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn watch",
    "build": "run-p build:*",
    "build:content": "eleventy",
    "build:css": "sass src:dist",
    "watch": "run-p watch:*",
    "watch:content": "eleventy --serve --quiet",
    "watch:css": "sass --watch src:dist",
    "lint": "run-s lint:js lint:css lint:prettier lint:webc",
    "lint:check": "run-s lint:*:check",
    "lint:js": "eslint . --fix",
    "lint:js:check": "eslint .",
    "lint:css": "stylelint --fix '**/*.scss'",
    "lint:css:check": "stylelint '**/*.scss'",
    "lint:prettier": "prettier . --write",
    "lint:prettier:check": "prettier . --check",
    "lint:webc": "prettier '**/*.webc' --write",
    "lint:webc:check": "prettier '**/*.webc' --check"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-img": "3.1.8",
    "@11ty/eleventy-navigation": "0.3.5",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "eleventy-plugin-time-to-read": "1.3.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.0.0",
    "luxon": "3.4.4",
    "markdown-it-footnote": "3.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.0",
    "sass": "1.69.5",
    "stylelint": "15.11.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-spaceninja": "14.0.0"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-webc": "0.11.1"
  }
}
